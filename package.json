{
  "name": "treeshaking-playground",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "clean": "rimraf dist",
    "build:esbuild": "yarn clean && tsx esbuild.config.ts",
    "build:rollup": "yarn clean && NODE_OPTIONS=--import=tsx rollup -c rollup.config.ts",
    "build:webpack": "yarn clean && NODE_OPTIONS=--import=tsx webpack --config webpack.config.ts",
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/register": "^7.24.6",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7",
    "@types/babel__register": "^7",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-loader": "^9.1.3",
    "esbuild": "^0.21.4",
    "esbuild-loader": "^4.1.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reselect": "^5.1.0"
  },
  "packageManager": "yarn@4.2.2"
}
