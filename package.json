{
  "name": "treeshaking-playground",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "clean": "rimraf dist",
    "build:esbuild": "yarn clean && tsx esbuild.config.ts",
    "build:rollup": "NODE_OPTIONS=--import=tsx rollup --config rollup.config.ts",
    "build:webpack": "yarn clean && NODE_OPTIONS=--import=tsx webpack --config webpack.config.ts",
    "build:vite": "yarn clean && vite build",
    "build:all": "yarn build:esbuild && yarn build:rollup && yarn build:webpack && yarn build:vite",
    "build:altogether": "yarn clean && tsx esbuild.config.ts && NODE_OPTIONS=--import=tsx rollup -c rollup.config.ts && NODE_OPTIONS=--import=tsx webpack --config webpack.config.ts && vite build",
    "start": "NODE_OPTIONS=--import='tsx/esm' node scripts/everyImport.mts"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/register": "^7.25.7",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.9.7",
    "@types/babel__register": "^7.17.3",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@vitejs/plugin-react": "^4.3.2",
    "babel-loader": "^9.2.1",
    "esbuild": "^0.24.0",
    "esbuild-loader": "^4.2.2",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "reselect": "^5.1.1"
  },
  "packageManager": "yarn@4.5.0"
}
